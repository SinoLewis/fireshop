{
  "name": "fireshop",
  "version": "1.0.0",
  "description": "Elevate your style and upgrade your shopping experience with edgy tech! ",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "build": "vite build && hugo",
    "dev": "concurrently \"npm run svelte:build\" \"npm run hugo:serve\"",
    "svelte": "vite",
    "svelte:build": "vite build --watch",
    "hugo:build": "hugo",
    "hugo:serve": "hugo server --forceSyncStatic --minify --bind=0.0.0.0 --port 6969",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "keywords": [],
  "author": "Lewis Nzau",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tailwindcss/typography": "^0.5.4",
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.23.0",
    "flamethrower-router": "^0.0.0-meme.12",
    "js-confetti": "^0.10.2",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.16",
    "sass": "^1.54.4",
    "svelte": "^3.49.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^3.0.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.23.0",
    "@upstash/redis": "^1.21.0",
    "crypto-js": "^4.1.1",
    "daisyui": "^4.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "meilisearch": "^0.30.0",
    "revolt.js": "7.0.0-beta.9",
    "uuid": "^9.0.0"
  }
}
