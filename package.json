{
  "name": "fireshop",
  "version": "1.0.0",
  "description": "Elevate your style and upgrade your shopping experience with edgy tech! ",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "svelte": "vite",
    "build": "vite build && hugo",
    "wbuild": "vite build --watch",
    "hugo": "hugo server --forceSyncStatic --minify --bind=0.0.0.0 --port 6969",
    "dev-hugo": "concurrently \"npm run wbuild\" \"npm run hugo \"",
    "dev": "concurrently \"npm run wbuild\" \"npm run hugo\" \"npm run meili\"",
    "prod": "concurrently \"npm run build \" \"npm run hugo \" \"npm run meili\"",
    "server-osm": "node server/osm.js",
    "server-meili": "meilisearch",
    "server-discord": "node server/discord.js",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "keywords": [],
  "author": "Lewis Nzau",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tailwindcss/typography": "^0.5.4",
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.23.0",
    "flamethrower-router": "^0.0.0-meme.12",
    "js-confetti": "^0.10.2",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.16",
    "sass": "^1.54.4",
    "svelte": "^3.49.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^3.0.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.2.0",
    "axios": "^1.3.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "daisyui": "^2.49.0",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "intl-tel-input": "^17.0.19",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.28",
    "jspdf-html2canvas": "^1.4.9",
    "mapbox-gl": "^2.11.1",
    "meilisearch": "^0.30.0",
